bool bitwise(int a, int b){
	if (a == 0 || b == 0) return true;
	int binary1[32];
	int binary2[32];
	
	int i = 0;
	int j = 0;
	while(a > 0 && b > 0){
		binary1[i++] = a % 2;
		binary2[j++] = b % 2;
		a = a / 2;
		b = b / 2;
	}
	int result[32];
	int x; 
	for(i = 0 , j = 0; i <32, j < 32; j++, i++){
		result[x++] = binary1[i] * binary2[j];
	}
	
	for(x = 0; x < 32; x ++){
		if(result[x] == 1){
			return false;
			break;
		}
	}
	return true;
	
}